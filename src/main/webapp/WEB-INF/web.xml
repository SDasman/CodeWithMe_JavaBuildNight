<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Embedded Tomcat 7 Java app</display-name>
  <servlet>
      <servlet-name>EvenOdd</servlet-name>
      <servlet-class>com.codewithme.javabuildnight.EvenOddServlet</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>DogCatPicker</servlet-name>
      <servlet-class>com.codewithme.javabuildnight.DogCatPicker</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>EvenOdd</servlet-name>
      <url-pattern>/evenodd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>DogCatPicker</servlet-name>
      <url-pattern>/dogcatpicker</url-pattern>
  </servlet-mapping>
  
</web-app>
